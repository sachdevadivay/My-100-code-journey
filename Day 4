#include <stdio.h>

int main() {
    int a, b;

    printf("enter the value of a and b : ");
    scanf("%d %d", &a, &b);

    
    a = a + b;
    b = a - b;
    a = a - b;

    
    printf("After swap: %d %d", a, b);

    return 0;
}
#include <stdio.h>

int main() {
    int n, sum;

    printf("enter the value of n : ");
    scanf("%d", &n);

    
    sum = n * (n + 1) / 2;

    
    printf("Sum=%d", sum);

    return 0;
}
