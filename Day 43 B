//Q86: Check if a string is a palindrome.



#include <stdio.h>

int main() {
    char str[1000];
    int len = 0;

    
    fgets(str, sizeof(str), stdin);

    
    while(str[len] != '\0' && str[len] != '\n') {
        len++;
    }

    
    int i = 0, j = len - 1;
    while(i < j) {
        if(str[i] != str[j]) {
            printf("Not palindrome");
            return 0;
        }
        i++;
        j--;
    }

    printf("Palindrome");

    return 0;
}
