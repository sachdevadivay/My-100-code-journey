//Q97: Print the initials of a name.



#include <stdio.h>

int main() {
    char str[100];
    fgets(str,100,stdin);

    
    printf("%c.", str[0]);

    
    for(int i = 1; str[i] != '\0'; i++) {
        if(str[i] == ' ' && str[i+1] != '\0' && str[i+1] != '\n') {
            printf("%c.", str[i+1]);
        }
    }

    return 0;
}
