#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    int A[100][100];
    int sum = 0;

    
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++) {
            scanf("%d", &A[i][j]);
        }
    }

  
    if(n == m) {
        for(int i = 0; i < n; i++) {
            sum += A[i][i];
        }
    }

    printf("%d", sum);

    return 0;
}
