//Q94: Find the longest word in a sentence.



#include <stdio.h>

int main() {
    char str[200];
    char longest[200];
    int maxLen = 0, currLen = 0, li = 0;

    
    fgets(str, sizeof(str), stdin);

    for (int i = 0; ; i++) {
        if (str[i] != ' ' && str[i] != '\0' && str[i] != '\n') {
            currLen++;
        } else {
            
            if (currLen > maxLen) {
                maxLen = currLen;
                int start = i - currLen;
                li = 0;
                while (li < currLen) {
                    longest[li] = str[start + li];
                    li++;
                }
                longest[li] = '\0';
            }
            currLen = 0;
        }

        if (str[i] == '\0')
            break;
    }

    printf("%s", longest);
    return 0;
}
