#include <stdio.h>

int main() {
    int a, b, sum;

    printf("enter the value of a and b : ");
    scanf("%d %d", &a, &b);

    
    sum = a + b;

    
    printf("Sum = %d\n", sum);

    return 0;
}
#include <stdio.h>

int main() {
    int a, b;
    int sum, diff, product, quotient;

    printf("enter the value of a and b : ");
    scanf("%d %d", &a, &b);

    
    sum = a + b;
    diff = a - b;
    product = a * b;
    quotient = a / b; 

    
    printf("Sum=%d, Diff=%d, Product=%d, Quotient=%d\n", sum, diff, product, quotient);

    return 0;
}

