//Q53: Write a program to print the following pattern:
*
***
*****
*******
*********
*******
*****
***
*



#include <stdio.h>

int main() {

    int stars;

    
    for (stars = 1; stars <= 9; stars += 2) {
        for (int j = 1; j <= stars; j++) {
            printf("*");
        }
        printf("\n");
    }

    
    for (stars = 7; stars >= 1; stars -= 2) {
        for (int j = 1; j <= stars; j++) {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
