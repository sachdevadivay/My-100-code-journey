//Q79: Perform diagonal traversal of a matrix.


#include <stdio.h>

int main() {
    int n, m;
    scanf("%d %d", &n, &m);

    int A[100][100];

    
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++) {
            scanf("%d", &A[i][j]);
        }
    }

    
    for(int startCol = 0; startCol < m; startCol++) {
        int r = 0, c = startCol;

        while(r < n && c >= 0) {
            printf("%d ", A[r][c]);
            r++;
            c--;
        }
    }

   
    for(int startRow = 1; startRow < n; startRow++) {
        int r = startRow, c = m - 1;

        while(r < n && c >= 0) {
            printf("%d ", A[r][c]);
            r++;
            c--;
        }
    }

    return 0;
}
