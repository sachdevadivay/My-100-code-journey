#include <stdio.h>
#include <ctype.h>

int main() {
    char str[1000];
    int vowels = 0, consonants = 0;
    int i = 0;

    
    fgets(str, sizeof(str), stdin);

    
    while(str[i] != '\0' && str[i] != '\n') {
        char ch = tolower(str[i]);

        if(ch >= 'a' && ch <= 'z') {  
            if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u')
                vowels++;
            else
                consonants++;
        }
        i++;
    }

    printf("Vowels=%d, Consonants=%d", vowels, consonants);

    return 0;
}
