#include <stdio.h>

int main() {
    int n, i;
    long long product = 1;  
    int foundEven = 0;      
    
    
    printf("Enter n: ");
    scanf("%d", &n);
    
    
    for (i = 2; i <= n; i += 2) {
        product *= i;
        foundEven = 1;
    }
    
    if (foundEven)
        printf("Product = %lld\n", product);
    else
        printf("No even numbers in range\n");
    
    return 0;
}
