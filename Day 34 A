#include <stdio.h>

int main() {
    int n, pos, x;
    scanf("%d", &n);

    int arr[100];   

    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    scanf("%d %d", &pos, &x); 
    

    
    for (int i = n - 1; i >= pos; i--) {
        arr[i + 1] = arr[i];
    }

    
    arr[pos] = x;
    n++;

    
    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
