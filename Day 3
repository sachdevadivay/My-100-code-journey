#include <stdio.h>

int main() {
    float celsius, fahrenheit;

    printf("enter the value of temperature in celsius : ");
    scanf("%f", &celsius);

    
    fahrenheit = (celsius * 9 / 5) + 32;

    
    printf("Fahrenheit=%.0f", fahrenheit);

    return 0;
}
#include <stdio.h>

int main() {
    int a, b, temp;

    printf("enter the value of a and b : ");
    scanf("%d %d", &a, &b);

    
    temp = a;
    a = b;
    b = temp;

    
    printf("After swap: %d %d", a, b);

    return 0;
}
